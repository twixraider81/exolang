int $a = 0;
int $b = 525;
int $c = 500;
int $d = 5;

int function puts( string $str );
int function printf( string $str ... );

$a += 1;
$b -= 500;
$d *= $c;
$c /= 5;

printf( "$a: %i\n", $a );
printf( "$b: %i\n", $b );
printf( "$c: %i\n", $c );
printf( "$d: %i\n", $d );

while( $a < $b ) {
	$a += 1;
	printf( "%i\n", $a );
};

printf( "%i\n", $a );

int $f = 0;
while( true ) {
	$f += 1;
	printf( "% i\n", $f );

	if( $f > 5 ) {
		break;
	};
};

