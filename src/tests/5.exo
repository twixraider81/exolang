int function puts( string $str );
int function printf( string $str ... );

class Object
{
	public	string	$typeName;

	public string function getTypeName()
	{
		$this->typeName = "Object";
		return $this->typeName;
	};

	public int function getInt( int $int )
	{
		return $int;
	};
};

class Boolean extends Object
{
	public boolean $value;

	public boolean function getValue()
	{
		return true;
	};

	public boolean function setValue( Boolean $newValue )
	{
		return true;
	};
};

class Integer extends Boolean
{
	public	integer $value;
};

class Float extends Integer
{
	public	integer $value;
};

Object $o = new Object();
Boolean $b = new Boolean();
Integer $i = new Integer();
Float $f = new Float();

int function getInt( int $int )
{
	return $int;
};

int $t = 1;
int $h = $o->getInt( $t );
int $j = getInt( $t );

// corrupt
// $i = $o->getInt( 1 );
printf( "$o->getInt() = %i", $h );
puts("");
printf( "getInt() = %i", $j );
puts("");
puts( $o->getTypeName() );

delete $o;
delete $b;
