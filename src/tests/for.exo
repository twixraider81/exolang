int function printf( string $str ... );

for( int $i = 1; $i < 10; $i += 1 ) {
	printf( "%i\n", $i );

	if( $i >= 5 ) {
		break;
	};
};



for( int $j = 1; $j < 10; $j += 1 ) {
	if( $j < 5 ) {
		printf( "continue\n" );
		continue;
	};

	if( $j >= 5 ) {
		printf( "break\n" );
		break;
	};
};


for( int $i = 6, int $j = 10; $i <= 10; $i += 1, $j -= 1 ) printf( "%i:%i\n", $i, $j );